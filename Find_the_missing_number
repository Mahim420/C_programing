#include <stdio.h>

int main()
{
    int test;
    scanf("%d", &test);

    while (test--)
    {
        int flag = -1;
        int num1, num2, num3;
        long long res;
        scanf("%lld %d %d %d", &res, &num1, &num2, &num3);

        if (res % (num1 * num2 * num3) == 0)
        {
            printf("%d", res / (num1 * num2 * num3));
        }
        else{
            printf("%d", flag);
        }

        printf("\n");
    }

    return 0;
}
