LINK : https://codeforces.com/group/MWSDmqGsZm/contest/326175/problem/C

#include<stdio.h>

int main(){
    char ch;
    printf("Enter charecter: ");
    scanf("%c", &ch);
    if(ch>=65 && ch<=90){
        printf("%c",ch+32);
    }
    else if(ch<=122 && ch>=97){
        printf("%c",ch-32);
    }
    return 0;
}
