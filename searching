#include<stdio.h>
 
int main(){
    int n;
    scanf("%d", &n);
 
    long long A[n], X;
 
    for(int i=0; i<n; i++){
        scanf("%lld", &A[i]);
    }
 
    scanf("%lld", &X);
    int result;
 
    for (int i = 0; i < n; i++)
    {
        if(A[i]==X){
            result = i;
            break;
        }
        else{
            result = -1;
        }
    }
 
    printf("%d", result);
    
 
    return 0;
}
