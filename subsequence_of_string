#include <stdio.h>
#include <string.h>
 
int main()
{
    char str[100] = "hello";
    char s[10002];
 
    int match = 0;
    scanf("%s", s);
 
    int len = strlen(s);
 
    for (int i = 0; i < len; i++)
    {
        if (str[match] == s[i])
        {
            match++;
        }
        
        if(match==5){
        	printf("YES\n");
          return 0;
        }
        
    }
 
    printf("NO");
 
 
 return 0;
 
}
 
