#include<stdio.h>
 
int main(){
    int n, r;
    scanf("%d", &n);
 
    int x[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &x[i]);
    }
 
    for(int i = 0; i<n; i++){
        if(x[i]>0){
            r = 1;
        }
        else if (x[i]==0)
        {
            r = 0;
        }
        else{
            r = 2;
        }
 
        printf("%d ", r);
        
    }
    
 
    return 0;
}
