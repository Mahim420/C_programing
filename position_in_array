#include<stdio.h>
 
int main(){
    int n, result;
    scanf("%d", &n);
 
    int x[n];
    for(int i=0; i<n; i++){
        scanf("%d", &x[i]);
    }
 
    for(int i=0; i<n; i++){
        if(x[i]<=10){
            result = i;
             printf("A[%d] = %d\n", result,x[i]);
        }
       
    }
    return 0;
}
