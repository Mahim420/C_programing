// https://codeforces.com/group/MWSDmqGsZm/contest/219856/problem/G

#include <stdio.h>
#include <string.h>

int main()
{
  char str[100001];

  int len;

  scanf("%s", str);

  len = strlen(str);
  // printf("%d", len);

  for (int i = 0; i < len; i++)
  {

    if (str[i] == ',')
    {
      str[i] = ' ';
      printf("%c", str[i]);
    }

    else if (str[i] >= 'a' && str[i] <= 'z')
    {
      // str[i] - 32;
      printf("%c", str[i]-32);
    }
    else
    {
      // str[i] + 32;
       printf("%c", str[i]+32);
    }
   
  }

  //

  return 0;
}

