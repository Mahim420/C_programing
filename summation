#include<stdio.h>
 
int main(){
    long long n, sum=0, result;
    scanf("%lld", &n);
 
 
    long long arr[n];
    for(int i=0; i<n; i++){
        scanf("%lld", &arr[i]);
    }
 
    for(int i=0; i<n; i++){
      result = sum+=arr[i];
    }
 
    if(result<0){
         printf("%lld", result*-1);
    }
 
    else{
        printf("%lld", result);
    }
   
 
    return 0;
}
