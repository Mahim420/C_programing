#include <bits/stdc++.h>
using namespace std;
int main()
{
    string str;
    cin >> str;

    int p = 0;

    for (int i = 0; i < str.size(); i++)
    {
        int j = i + 1, k = j + 1, l = k + 1, m = l + 1;
        if (str[i] == 'E' && str[j] == 'G' && str[k] == 'Y' && str[l] == 'P' && str[m] == 'T')
        {
            str.replace(i, 5, " ");
        }
    }

    cout << str;

    return 0;
}
